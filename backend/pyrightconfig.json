{
    "venvPath": ".",
    "venv": "venv",
    "typeCheckingMode": "basic",
    "include": [
        "api/permissions.py",
        "api/viewsets.py",
        "api/routers.py",
        "api/__init__.py",
        "core/settings.py",
        "core/urls.py"
    ],
    "exclude": [
        "**/migrations/**",
        "api/models.py",
        "api/views.py",
        "api/schema.py",
        "api/routing.py",
        "create_sample_activities.py",
        "show_sample_data.py",
        "setup_database.py",
        "setup_database 2.py",
        "test_*",
        "*_test.py"
    ],
    "extraPaths": [
        "./venv/lib/python3.13/site-packages"
    ],
    "reportIncompatibleMethodOverride": "none",
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.13",
            "pythonPlatform": "Darwin",
            "extraPaths": [
                "./venv/lib/python3.13/site-packages"
            ]
        }
    ]
}
